{
  "name": "finopsguard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "finopsguard-github": "dist/integrations/github/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "NODE_ENV=development tsx src/api/server.ts",
    "start": "NODE_ENV=production node dist/api/server.js",
    "test": "vitest run",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\" \"tests/**/*.{ts,tsx}\"",
    "post-to-pr": "tsx src/integrations/github/cli.ts"
  },
  "dependencies": {
    "express": "^4.19.2",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.14.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "supertest": "^7.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
}
